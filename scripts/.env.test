# MCP Installation Testing Environment Configuration

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# Test Configuration
NODE_ENV=test
TEST_USER_ID=test-user-id-here
MAX_SERVERS_TO_TEST=10
DELAY_BETWEEN_TESTS=3000
MAX_RETRIES=2
TEST_TIMEOUT=120000

# Supported Installation Methods
SUPPORTED_METHODS=npx,python

# Report Configuration
GENERATE_HTML_REPORT=true
REPORTS_DIRECTORY=../reports

# Performance Thresholds (milliseconds)
MAX_INSTALL_TIME=60000
MAX_DB_VERIFICATION_TIME=5000
MAX_UNINSTALL_TIME=30000

# Quality Thresholds
MINIMUM_SUCCESS_RATE=70

# Debug Options
DEBUG_MODE=false
VERBOSE_LOGGING=true
SAVE_INSTALLATION_LOGS=true